<form
    addfieldpath="/components/com_jtca/models/fields"
    addrulepath="/components/com_jtca/models/rules"
>
    <fieldset>
        <field
			id="id"
			name="id"
			type="hidden"
			label="COM_JTCA_ID_LABEL"
			class="readonly"
			default="0"
			readonly="true"
		/>

        <field
			id="contenthistory"
			name="contenthistory"
			type="contenthistory"
			label="JTOOLBAR_VERSIONS"
			data-typeAlias="com_jtca.slpartecontenciosa"
		/>

        <field
			id="created"
			name="created"
			type="calendar"
			label="COM_JTCA_FIELD_CREATED_LABEL"
			description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CREATED_BY_DESC"
			class="inputbox"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"		
		/>
        <field
			id="created_by"
			name="created_by"
			type="user"
			label="COM_JTCA_FIELD_CREATED_BY_LABEL"
			description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CREATED_BY_DESC"
		/>
        <field
            id="modified"
            name="modified"
            type="calendar"
			label="COM_JTCA_FIELD_MODIFIED_LABEL"
			description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_MODIFIED_DESC"
            class="readonly"
			size="22"
			readonly="true"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
        <field
            id="modified_by"
            name="modified_by"
            type="user"
		    label="COM_JTCA_FIELD_MODIFIED_BY_LABEL"
		    class="readonly"
		    readonly="true"
		    filter="unset"
		/>
        <field
            id="state"
            name="state"
            type="list"
            label="JSTATUS"
            description="JFIELD_PUBLISHED_DESC"
			class="chzn-color-state"
            size="1"
            default="1"
	    >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASH</option>
        </field>
        <field
			id="version_note"
			name="version_note"
			type="text"
			label="JGLOBAL_FIELD_VERSION_NOTE_LABEL"
			description="JGLOBAL_FIELD_VERSION_NOTE_DESC"
			class="inputbox"
			size="45"
		/>
    </fieldset>
    <fieldset
        name="fieldset_partescontenciosas_fs"
        label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELDSET_PARTESCONTENCIOSAS_FS_LABEL"
    >

        <field
            id="txt_ijuridico"
            name="txt_ijuridico"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_TXT_IJURIDICO_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_TXT_IJURIDICO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="id_ijuridico"
            name="id_ijuridico"
            type="combo_sql"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_ID_IJURIDICO_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_ID_IJURIDICO_DESC"
        >
        </field>  
        <field
            id="isMoral"
            name="isMoral"
            type="checkbox"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_ISMORAL_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_ISMORAL_DESC"
            class="inputbox"
			value="1"
        >
        </field>  
        <field
            id="paterno"
            name="paterno"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_PATERNO_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_PATERNO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="materno"
            name="materno"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_MATERNO_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_MATERNO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="nombre"
            name="nombre"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_NOMBRE_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_NOMBRE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="curp"
            name="curp"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CURP_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CURP_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="rfc"
            name="rfc"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_RFC_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_RFC_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="calle"
            name="calle"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CALLE_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CALLE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="numero"
            name="numero"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_NUMERO_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_NUMERO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="colonia"
            name="colonia"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_COLONIA_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_COLONIA_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="cp"
            name="cp"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CP_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_CP_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="id_entidadf"
            name="id_entidadf"
            type="combo_sql"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_ID_ENTIDADF_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_ID_ENTIDADF_DESC"
        >
        </field>  
        <field
            id="municipio"
            name="municipio"
            type="text"
            label="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_MUNICIPIO_LABEL"
            description="COM_JTCA_SLPARTESCONTENCIOSAS_FIELD_MUNICIPIO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
    </fieldset>
</form>
