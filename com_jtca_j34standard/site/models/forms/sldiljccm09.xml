<form
    addfieldpath="/components/com_jtca/models/fields"
    addrulepath="/components/com_jtca/models/rules"
>
    <fieldset>
        <field
			id="id"
			name="id"
			type="hidden"
			label="COM_JTCA_ID_LABEL"
			class="readonly"
			default="0"
			readonly="true"
		/>

        <field
			id="contenthistory"
			name="contenthistory"
			type="contenthistory"
			label="JTOOLBAR_VERSIONS"
			data-typeAlias="com_jtca.sldiljccm09"
		/>

        <field
			id="created"
			name="created"
			type="calendar"
			label="COM_JTCA_FIELD_CREATED_LABEL"
			description="COM_JTCA_SLDIL_JCCM09S_FIELD_CREATED_BY_DESC"
			class="inputbox"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"		
		/>
        <field
			id="created_by"
			name="created_by"
			type="user"
			label="COM_JTCA_FIELD_CREATED_BY_LABEL"
			description="COM_JTCA_SLDIL_JCCM09S_FIELD_CREATED_BY_DESC"
		/>
        <field
            id="modified"
            name="modified"
            type="calendar"
			label="COM_JTCA_FIELD_MODIFIED_LABEL"
			description="COM_JTCA_SLDIL_JCCM09S_FIELD_MODIFIED_DESC"
            class="readonly"
			size="22"
			readonly="true"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
        <field
            id="modified_by"
            name="modified_by"
            type="user"
		    label="COM_JTCA_FIELD_MODIFIED_BY_LABEL"
		    class="readonly"
		    readonly="true"
		    filter="unset"
		/>
        <field
            id="state"
            name="state"
            type="list"
            label="JSTATUS"
            description="JFIELD_PUBLISHED_DESC"
			class="chzn-color-state"
            size="1"
            default="1"
	    >
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASH</option>
        </field>
        <field
			id="version_note"
			name="version_note"
			type="text"
			label="JGLOBAL_FIELD_VERSION_NOTE_LABEL"
			description="JGLOBAL_FIELD_VERSION_NOTE_DESC"
			class="inputbox"
			size="45"
		/>
    </fieldset>
    <fieldset
        name="fieldset_dil_jccm09_fs"
        label="COM_JTCA_SLDIL_JCCM09S_FIELDSET_DIL_JCCM09_FS_LABEL"
    >

        <field
            id="sfield5"
            name="sfield5"
            type="text"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD5_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD5_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="sfield8_isMoral"
            name="sfield8_isMoral"
            type="person"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_ISMORAL_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_ISMORAL_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="sfield8_paterno"
            name="sfield8_paterno"
            type="person"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_PATERNO_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_PATERNO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="sfield8_materno"
            name="sfield8_materno"
            type="person"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_MATERNO_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_MATERNO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="sfield8_nombre"
            name="sfield8_nombre"
            type="person"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_NOMBRE_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD8_NOMBRE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="sfield9"
            name="sfield9"
            type="editor"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD9_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD9_DESC"
            width="100%"
			height="250"
			cols="50"
			rows="5"
			buttons="*"
			hide_buttons="*"
			default=""
        >
        </field>  
        <field
            id="sfield10"
            name="sfield10"
            type="calendar"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD10_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD10_DESC"
            class="inputbox validate-date"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="fixed"
			validate="date"
        >
        </field>  
        <field
            id="sfield11"
            name="sfield11"
            type="calendar"
            label="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD11_LABEL"
            description="COM_JTCA_SLDIL_JCCM09S_FIELD_SFIELD11_DESC"
            class="inputbox validate-date"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="fixed"
			validate="date"
        >
        </field>  
    </fieldset>
</form>
