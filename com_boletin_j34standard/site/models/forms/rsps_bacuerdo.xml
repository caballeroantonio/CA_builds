        </field>  
        <field
            id="origen"
            name="origen"
            type="text"
            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_ORIGEN_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_ORIGEN_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="paterno"
            name="paterno"
            type="text"
            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_PATERNO_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_PATERNO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="materno"
            name="materno"
            type="text"
            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_MATERNO_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_MATERNO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="nombre"
            name="nombre"
            type="text"
            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_NOMBRE_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_NOMBRE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
    </fieldset>
</form>
