<form
    addfieldpath="/components/com_boletin/models/fields"
    addrulepath="/components/com_boletin/models/rules"
>
    <fieldset>
        <field
			id="id"
			name="id"
			type="hidden"
			label="COM_BOLETIN_ID_LABEL"
			class="readonly"
			default="0"
			readonly="true"
		/>


    </fieldset>
    <fieldset
        name="fieldset_rsps_bacuerdo_fs"
        label="COM_BOLETIN_RSPS_BACUERDOS_FIELDSET_RSPS_BACUERDO_FS_LABEL"
    >

        <field
            id="expediente"
            name="expediente"
            type="text"
            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_EXPEDIENTE_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_EXPEDIENTE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="sancionadora"
            name="sancionadora"
            type="text"
            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_SANCIONADORA_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_SANCIONADORA_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
