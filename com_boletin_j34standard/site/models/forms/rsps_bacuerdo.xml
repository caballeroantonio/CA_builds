            label="COM_BOLETIN_RSPS_BACUERDOS_FIELD_F_RESOLUCION_LABEL"
            description="COM_BOLETIN_RSPS_BACUERDOS_FIELD_F_RESOLUCION_DESC"
            class="inputbox"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="user_utc"
