<form
    addfieldpath="/components/com_boletin/models/fields"
    addrulepath="/components/com_boletin/models/rules"
>
    <fieldset>
        <field
			id="id"
			name="id"
			type="hidden"
			label="COM_BOLETIN_ID_LABEL"
			class="readonly"
			default="0"
			readonly="true"
		/>


        <field
		    id="description"
		    name="description"
			type="editor"
			label="JGLOBAL_DESCRIPTION"
			description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_DESCRIPTION_DESC"
			class="inputbox"
            filter="JComponentHelper::filterText"
			buttons="true"
            hide="article,pagebreak"                
		/>
    </fieldset>
    <fieldset
        name="fieldset_tsjcdmx_sala_acuerdo_fs"
        label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELDSET_TSJCDMX_SALA_ACUERDO_FS_LABEL"
    >

        <field
            id="fecha_acuerdo"
            name="fecha_acuerdo"
            type="calendar"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_FECHA_ACUERDO_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_FECHA_ACUERDO_DESC"
            class="inputbox"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="user_utc"
			required="true"
        >
        </field>  
        <field
            id="fecha_boletin"
            name="fecha_boletin"
            type="calendar"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_FECHA_BOLETIN_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_FECHA_BOLETIN_DESC"
            class="inputbox"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="user_utc"
			required="true"
        >
        </field>  
        <field
            id="sala"
            name="sala"
            type="text"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_SALA_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_SALA_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="actor"
            name="actor"
            type="text"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_ACTOR_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_ACTOR_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="demandado"
            name="demandado"
            type="text"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_DEMANDADO_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_DEMANDADO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="terceria"
            name="terceria"
            type="text"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TERCERIA_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TERCERIA_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="tipo_juicio"
            name="tipo_juicio"
            type="text"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TIPO_JUICIO_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TIPO_JUICIO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="toca"
            name="toca"
            type="integer"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TOCA_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TOCA_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field
            id="anio"
            name="anio"
            type="integer"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_ANIO_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_ANIO_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field
            id="asunto"
            name="asunto"
            type="integer"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_ASUNTO_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_ASUNTO_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field
            id="tipo_resolucion"
            name="tipo_resolucion"
            type="text"
            label="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TIPO_RESOLUCION_LABEL"
            description="COM_BOLETIN_TSJCDMX_SALA_ACUERDOS_FIELD_TIPO_RESOLUCION_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
    </fieldset>
</form>
