<form
    addfieldpath="/components/com_boletin/models/fields"
    addrulepath="/components/com_boletin/models/rules"
>
    <fieldset>
        <field
			id="id"
			name="id"
			type="hidden"
			label="COM_BOLETIN_ID_LABEL"
			class="readonly"
			default="0"
			readonly="true"
		/>


    </fieldset>
    <fieldset
        name="fieldset_srsps_bacuerdo_fs"
        label="COM_BOLETIN_SRSPS_BACUERDOS_FIELDSET_SRSPS_BACUERDO_FS_LABEL"
    >

        <field
            id="rfc"
            name="rfc"
            type="text"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_RFC_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_RFC_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="nombre"
            name="nombre"
            type="text"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_NOMBRE_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_NOMBRE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="oficio"
            name="oficio"
            type="text"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_OFICIO_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_OFICIO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="fecha_oficio"
            name="fecha_oficio"
            type="calendar"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_FECHA_OFICIO_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_FECHA_OFICIO_DESC"
            class="inputbox"
			size="22"
			maxlength="25"
			format="Y-m-d"
			filter="user_utc"
			required="true"
        >
        </field>  
        <field
            id="emisor"
            name="emisor"
            type="text"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_EMISOR_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_EMISOR_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="id_medio_notificacion"
            name="id_medio_notificacion"
            type="list"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_ID_MEDIO_NOTIFICACION_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_ID_MEDIO_NOTIFICACION_DESC"
            class="inputbox"
			size="1"
        >
            <option	value="Estrados de la autoridad">COM_BOLETIN_SRSPS_BACUERDOS_ID_MEDIO_NOTIFICACION_VALUE_ESTRADOS_DE_LA_AUTORIDAD</option>
			<option	value="Notificación personal">COM_BOLETIN_SRSPS_BACUERDOS_ID_MEDIO_NOTIFICACION_VALUE_NOTIFICACION_PERSONAL</option>
			<option	value="Notificación por Buzón Tributario">COM_BOLETIN_SRSPS_BACUERDOS_ID_MEDIO_NOTIFICACION_VALUE_NOTIFICACION_POR_BUZON_TRIBUTARIO</option>
        </field>  
        <field
            id="fecha_notificacion"
            name="fecha_notificacion"
            type="calendar"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_FECHA_NOTIFICACION_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_FECHA_NOTIFICACION_DESC"
            class="inputbox"
			size="22"
			maxlength="25"
			format="Y-m-d"
			filter="user_utc"
			required="true"
        >
        </field>  
        <field
            id="fecha_surte"
            name="fecha_surte"
            type="calendar"
            label="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_FECHA_SURTE_LABEL"
            description="COM_BOLETIN_SRSPS_BACUERDOS_FIELD_FECHA_SURTE_DESC"
            class="inputbox"
			size="22"
			maxlength="25"
			format="Y-m-d"
			filter="user_utc"
			required="true"
        >
        </field>  
    </fieldset>
</form>
