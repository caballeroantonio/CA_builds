<form
    addfieldpath="/administrator/components/com_boletin/models/fields"
    addrulepath="/administrator/components/com_boletin/models/rules"

>
    <fieldset name="basic_details"
    >
        <field name="id"
		    type="text"
            label="JGLOBAL_FIELD_ID_LABEL"
            description ="JGLOBAL_FIELD_ID_DESC"
			readonly="true"
			class="readonly"
            default="0"
		/>
        <field name="buttonspacer"
            type="spacer"
            label="JGLOBAL_ACTION_PERMISSIONS_LABEL"
            description="JGLOBAL_ACTION_PERMISSIONS_DESCRIPTION"
        />

    </fieldset>
    <fieldset name="fieldset_profeco_proveedor_fs"
        label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELDSET_PROFECO_PROVEEDOR_FS_LABEL"
        description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELDSET_PROFECO_PROVEEDOR_FS_DESC"
    >
        <field name="nombre"
            type="text"
            label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_NOMBRE_LABEL"
            description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_NOMBRE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field name="marca"
            type="text"
            label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_MARCA_LABEL"
            description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_MARCA_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field name="porcentaje"
            type="integer"
            label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_PORCENTAJE_LABEL"
            description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_PORCENTAJE_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field name="is_concilianet"
            type="checkbox"
            label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_IS_CONCILIANET_LABEL"
            description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_IS_CONCILIANET_DESC"
            class="inputbox"
			value="1"
        >
        </field>  
        <field name="is_conciliaexpres"
            type="checkbox"
            label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_IS_CONCILIAEXPRES_LABEL"
            description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_IS_CONCILIAEXPRES_DESC"
            class="inputbox"
			value="1"
        >
        </field>  
        <field name="id_giro"
            type="text"
            label="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_ID_GIRO_LABEL"
            description="COM_BOLETIN_PROFECO_PROVEEDORES_FIELD_ID_GIRO_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
    </fieldset>
    <fieldset name="publishing"
		label="COM_BOLETIN_FIELDSET_PUBLISHING_LABEL"
    >
    </fieldset>
</form>
