<form
    addfieldpath="/components/com_remca/models/fields"
    addrulepath="/components/com_remca/models/rules"
>
    <fieldset>
        <field
			id="id"
			name="id"
			type="hidden"
			label="COM_REMCA_ID_LABEL"
			class="readonly"
			default="0"
			readonly="true"
		/>

        <field
			id="contenthistory"
			name="contenthistory"
			type="contenthistory"
			label="JTOOLBAR_VERSIONS"
			data-typeAlias="com_remca.rent_sal"
		/>

        <field
			id="version_note"
			name="version_note"
			type="text"
			label="JGLOBAL_FIELD_VERSION_NOTE_LABEL"
			description="JGLOBAL_FIELD_VERSION_NOTE_DESC"
			class="inputbox"
			size="45"
		/>
    </fieldset>
    <fieldset
        name="fieldset_rent_sal_fs"
        label="COM_REMCA_RENT_SAL_FIELDSET_RENT_SAL_FS_LABEL"
    >

        <field
            id="id_house"
            name="id_house"
            type="modal_houses"
            label="COM_REMCA_RENT_SAL_FIELD_ID_HOUSE_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_ID_HOUSE_DESC"
            class="modal"
			link="1"
			default="0"
			type_alias="com_remca.house" 
        >
        </field>  
        <field
            id="monthW"
            name="monthW"
            type="integer"
            label="COM_REMCA_RENT_SAL_FIELD_MONTHW_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_MONTHW_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field
            id="yearW"
            name="yearW"
            type="integer"
            label="COM_REMCA_RENT_SAL_FIELD_YEARW_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_YEARW_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field
            id="week"
            name="week"
            type="text"
            label="COM_REMCA_RENT_SAL_FIELD_WEEK_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_WEEK_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="weekend"
            name="weekend"
            type="text"
            label="COM_REMCA_RENT_SAL_FIELD_WEEKEND_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_WEEKEND_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="midweek"
            name="midweek"
            type="text"
            label="COM_REMCA_RENT_SAL_FIELD_MIDWEEK_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_MIDWEEK_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="price_from"
            name="price_from"
            type="calendar"
            label="COM_REMCA_RENT_SAL_FIELD_PRICE_FROM_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_PRICE_FROM_DESC"
            class="inputbox validate-date"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="fixed"
			validate="date"
        >
        </field>  
        <field
            id="price_to"
            name="price_to"
            type="calendar"
            label="COM_REMCA_RENT_SAL_FIELD_PRICE_TO_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_PRICE_TO_DESC"
            class="inputbox validate-date"
			size="22"
			maxlength="25"
			format="%Y-%m-%d"
			filter="fixed"
			validate="date"
        >
        </field>  
        <field
            id="special_price"
            name="special_price"
            type="currency"
            label="COM_REMCA_RENT_SAL_FIELD_SPECIAL_PRICE_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_SPECIAL_PRICE_DESC"
            class="inputbox"
			size="1"
			default="0"
        >
        </field>  
        <field
            id="comment_price"
            name="comment_price"
            type="text"
            label="COM_REMCA_RENT_SAL_FIELD_COMMENT_PRICE_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_COMMENT_PRICE_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
        <field
            id="priceunit"
            name="priceunit"
            type="text"
            label="COM_REMCA_RENT_SAL_FIELD_PRICEUNIT_LABEL"
            description="COM_REMCA_RENT_SAL_FIELD_PRICEUNIT_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
    </fieldset>
</form>
