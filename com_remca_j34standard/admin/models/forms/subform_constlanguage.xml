<form
    addfieldpath="/administrator/components/com_remca/models/fields"
    addrulepath="/administrator/components/com_remca/models/rules"

>
    <fieldset name="basic_details"
    >
        <field name="buttonspacer"
            type="spacer"
            label="JGLOBAL_ACTION_PERMISSIONS_LABEL"
            description="JGLOBAL_ACTION_PERMISSIONS_DESCRIPTION"
        />
    </fieldset>
    <fieldset name="fieldset_const_languages_fs"
        label="COM_REMCA_CONST_LANGUAGE_FIELDSET_CONST_LANGUAGES_FS_LABEL"
        description="COM_REMCA_CONST_LANGUAGE_FIELDSET_CONST_LANGUAGES_FS_DESC"
    >
        <field name="fk_constid"
            type="modal_const"
            label="COM_REMCA_CONST_LANGUAGE_FIELD_FK_CONSTID_LABEL"
            description="COM_REMCA_CONST_LANGUAGE_FIELD_FK_CONSTID_DESC"
            class="modal"
			default="0"
			type_alias="com_remca.const" 
        >
        </field>  
        <field name="fk_languagesid"
            type="modal_languages"
            label="COM_REMCA_CONST_LANGUAGE_FIELD_FK_LANGUAGESID_LABEL"
            description="COM_REMCA_CONST_LANGUAGE_FIELD_FK_LANGUAGESID_DESC"
            class="modal"
			default="0"
			type_alias="com_remca.language" 
        >
        </field>  
        <field name="value_const"
            type="text"
            label="COM_REMCA_CONST_LANGUAGE_FIELD_VALUE_CONST_LABEL"
            description="COM_REMCA_CONST_LANGUAGE_FIELD_VALUE_CONST_DESC"
            class="inputbox"
			size="50"
			maxlength="255"
        >
        </field>  
    </fieldset>
    <fieldset name="publishing"
		label="COM_REMCA_FIELDSET_PUBLISHING_LABEL"
    >

    </fieldset>
</form>
